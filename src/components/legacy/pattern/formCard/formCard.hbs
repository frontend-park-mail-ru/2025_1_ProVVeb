<div class="formCard">
	<form action="" class="formCard__inner">
		<div class="formCard__header">
			{{{ progressBar }}}
			<h2>{{ cardTitle }}</h2>
		</div>
		<div class="formCard__fields">
			{{#each fields}}
			{{{ this }}}
			{{/each}}
		</div>
		<div class="formCard__buttonField">
			{{{ button }}}
			{{{ linkToPage }}}
		</div>
	</form>
</div>
