<div class="navMenu">
	<input type="checkbox" id="mobileMenuToggle" class="mobileMenuToggle__checkbox">
	<label for="mobileMenuToggle" class="mobileMenuToggle__button">
		<span></span>
		<span></span>
		<span></span>
	</label>
	<nav class="navMenu__inner">
		<div class="navMenu__links">
			{{#each links}}
			<div id="{{this.id}}" class="navMenu__link{{#if this.isDev}} navMenu__link--dev{{/if}}">
				<img src="{{this.srcIcon}}" alt="Иконка {{this.title}}">
				<span class="">{{this.title}}</span>
			</div>
			{{/each}}
		</div>
	</nav>
</div>
